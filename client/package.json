  "scripts": {
    "start": "DEV=true webpack-dev-server --history-api-fallback --config webpack/dev.js --progress",
    "start:sourcemap": "DEV=true SOURCE_MAP=true webpack-dev-server --history-api-fallback --config webpack/dev.js --progress",
    "start:dist": "NODE_ENV=production webpack-dev-server --config webpack/prod.js",
    "profile": "webpack --config webpack/dev.js --profile --json > stats.json",
    "profile:bundlesize:dll": "webpack --config webpack/dll.js --profile --json | webpack-bundle-size-analyzer > bundle-dll.out",
    "profile:bundlesize:prod": "NODE_ENV=production webpack --config webpack/prod.js --bail --profile --json > bundle-prod-stats.json",
    "bundle-size": "npm run build && ls dist/*.app.js | xargs -n 2 -I file source-map-explorer file file.map",
    "build": "npm run clean && NODE_ENV=production npm run build:prod",
    "build:clean-all": "npm run clean:all && NODE_ENV=production npm run build:dev",
    "build:dev": "webpack --config webpack/dev.js --progress",
    "build:prod": "webpack --config webpack/prod.js --progress",
    "build:loader": "webpack --config webpack/loader.js  --progress",
    "lint": "eslint . --ext .js --ext .jsx --fix",
    "deploy": "../../tools/deploy-ui.sh",
    "clean": "rm -rf  dist .tmp",
    "clean:all": "rm -rf node_modules  app/bower_components && npm run clean",
    "test": "DEV=true NODE_ENV=development npm run test:watch",
    "test:unit": "DEV=true NODE_ENV=development CHROME_BIN=/usr/bin/google-chrome-stable karma start ./karma/unit.conf.js",
    "test:watch": "CHROME_BIN=/usr/bin/google-chrome-stable karma start ./karma/watch.conf.js",
    "debug:webpack": "DEV=true node --inspect node_modules/.bin/webpack-dev-server --history-api-fallback --config webpack/dev.js",
    "translation:missingkeys": "node webpack/missing-messages.js"
  },
