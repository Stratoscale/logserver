{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true,
  },
  "plugins": [
    "react",
    "immutablejs"
  ],
  "globals": {
    "__DEV__": false,
    "__UI_VERSION__": false,
    "ANALYTICS_KEYS": false
  },
  "rules": {
    "no-console": 0,
    "new-cap": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "eol-last": 0,
    "comma-dangle": [1, "always-multiline"],
    "quotes": [1, "single"],
    "no-unused-vars": [1, {"vars": "local", "args": "none", "varsIgnorePattern": "[_]"}],
    "jsx-quotes": 1,
    "space-before-blocks": 1,
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "immutablejs/no-native-map-set": 2,
    "no-debugger": 1
  }
}
