{
  "sources": [
    {
      "name": "node1",
      "url": "file://./example/log1"
    },
    {
      "name": "node2",
      "url": "file://./example/log2",
      "open_tar_files": true
    },
    {
      "name": "node3",
      "url": "file://./example/log3",
      "open_tar_files": true
    }
  ],
  "parsers": [
    {
      "glob": "*.stratolog",
      "json_mapping": {
        "msg": "msg",
        "level": "levelname",
        "time": "created",
        "args": "args"
      },
      "time_formats": [
        "unix_float"
      ]
    },
    {
      "regexp": "(?P<timestamp>\\d{4}-\\d{2}-\\d{2}\\W\\d{2}:\\d{2}:\\d{2},\\d{3}.\\d{3})\\W\\d+\\W(?P<level>[[:alpha:]]+)\\W(?P<module>[^\\.]+)\\.(?P<function>[^\\W]+)\\W\\[[^]+]\\]\\W(?P<msg>.*)",
      "time_formats": [
        "2006-01-02 15:04:05.000"
      ]
    }
  ],
  "dynamic": {
    "root": "./"
  }
}
